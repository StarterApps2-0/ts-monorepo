{
	"name": "api",
	"version": "1.5.6",
	"main": "index.js",
	"author": "Ryan Oillataguerre",
	"license": "MIT",
	"scripts": {
		"start": "node build/src/index.js",
		"build": "tsc",
		"dev": "nodemon --config nodemon.json src/index.ts",
		"knex:migrate:make": "knex --knexfile ../shared/src/db/knexfile.ts migrate:make",
		"knex:migrate:latest": "knex --knexfile ../shared/src/db/knexfile.ts migrate:latest",
		"knex:migrate:up": "knex --knexfile ../shared/src/db/knexfile.ts migrate:up",
		"knex:migrate:down": "knex --knexfile ../shared/src/db/knexfile.ts migrate:down",
		"test": "jest --runInBand",
		"test:watch": "jest --watch",
		"test:coverage": "jest --coverage"
	},
	"devDependencies": {
		"@types/cookie-parser": "^1.4.2",
		"@types/express": "^4.17.13",
		"dotenv": "^10.0.0",
		"jest": "^27.3.1",
		"node-ts": "^5.1.2",
		"nodemon": "^2.0.7",
		"supertest": "^6.1.6",
		"ts-jest": "^27.0.7",
		"ts-node": "^10.0.0",
		"tslint": "^6.1.3",
		"typescript": "^4.3.5"
	},
	"dependencies": {
		"shared": "*",
		"cookie-parser": "^1.4.6",
		"express": "^4.17.1",
		"helmet": "^4.6.0",
		"nanoid": "^3.1.25"
	}
}
